{
  "containerDefinitions": [
    {
      "name": "app",
      "image": "115278533354.dkr.ecr.us-east-1.amazonaws.com/electnow-mastodon-staging:latest",
      "cpu": 0,
      "essential": true,
      "command": [
        "bash",
        "-c",
        "bundle exec rails db:migrate && bundle exec rails s -p 3000"
      ],
      "portMappings": [
        {
          "name": "webservice-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::mastodon-env-files/staging.env",
          "type": "s3"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/electnow-mastodon-staging/web",
          "awslogs-stream-prefix": "ecs",
          "awslogs-region": "ap-southeast-2"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:3000/health || exit 1"
        ],
        "interval": 60,
        "timeout": 30,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ],
  "family": "electnow-mastodon-staging",
  "taskRoleArn": "arn:aws:iam::115278533354:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::115278533354:role/ecsTaskRole",
  "networkMode": "awsvpc",
  "requiresAttributes": [
    {
      "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
    },
    {
      "name": "ecs.capability.execution-role-awslogs"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
    },
    {
      "name": "ecs.capability.secrets.asm.environment-variables"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.17"
    },
    {
      "name": "com.amazonaws.ecs.capability.task-iam-role"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
    },
    {
      "name": "ecs.capability.task-eni"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.29"
    }
  ],
  "compatibilities": ["EC2", "FARGATE"],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
